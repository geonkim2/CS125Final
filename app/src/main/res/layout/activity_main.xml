<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/MainActivityXMLFile"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="StepMobile"
        tools:layout_editor_absoluteX="176dp"
        tools:layout_editor_absoluteY="86dp" />

    <Button
        android:id="@+id/song_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="First Music"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="234dp" />

    <Button
        android:id="@+id/song_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Other Song"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="352dp" />

    <Button
        android:id="@+id/song_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Also Song"
        tools:layout_editor_absoluteX="248dp"
        tools:layout_editor_absoluteY="234dp" />

    <Button
        android:id="@+id/song_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Last Song"
        tools:layout_editor_absoluteX="247dp"
        tools:layout_editor_absoluteY="352dp" />
</android.support.constraint.ConstraintLayout>